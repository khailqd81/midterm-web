{
    "extends": ["react-app", "prettier"],
    // "env": {
    //   "browser": true,
    //   "es6": true
    // },
    "plugins": ["react", "prettier"],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "arrowParens": "always",
                "semi": true,
                "tabWidth": 4,
                "singleQuote": false,
                "jsxSingleQuote": false,
                "useTabs": false,
                "endOfLine": "auto"
            }
        ]
    }
}
